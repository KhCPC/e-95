// Оголошення змінних name та price let name = "Генератор захисного поля";
 let price = 1000;
// Виведення інформації про товар 
console.log(Обрано «${name}», ціна за штуку ${price} кредитів.);
// Зміна ціни товару
 price = 2000;
// Виведення оновленої інформації про товар
 console.log(Обрано «${name}», ціна за штуку ${price} кредитів.);
